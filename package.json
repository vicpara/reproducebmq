{
  "name": "emailagent",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "broadn",
  "main": "src/app.ts",
  "scripts": {
    "start": "bun run ./src/app.ts",
    "dev": "bun --watch run ./src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "commonjs",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.7.1",
    "bullmq": "^4.12.0",
    "ioredis": "^5.3.2",
    "redis": "^4.6.10"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.3",
    "@types/lodash.get": "^4.4.7",
    "bun-types": "^1.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/dist/$1"
    }
  },
  "prettier": {
    "cssEnable": [
      "css",
      "less",
      "scss",
      "postcss"
    ],
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxBracketSameLine": false,
    "singleQuote": true,
    "printWidth": 150,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "strict",
    "useTabs": false
  }
}